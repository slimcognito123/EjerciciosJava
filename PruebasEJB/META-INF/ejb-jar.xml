<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
         version="3.1">
    <enterprise-beans>
        <session>
            <ejb-name>SessionEJB</ejb-name>
            <ejb-class>negocio.PrimerEJB</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Bean</transaction-type>
        </session>
        <session>
            <ejb-name>SessionSegundoEJEJB</ejb-name>
            <ejb-class>SessionSegundoEJBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Bean</transaction-type>
        </session>
        <entity>
            <ejb-name>NombreEJB</ejb-name>
            <home>negocio.NombreHome</home>
            <remote>negocio.Nombre</remote>
            <local-home>negocio.LocalNombreHome</local-home>
            <local>negocio.LocalNombre</local>
            <ejb-class>negocio.NombreBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.String</prim-key-class>
            <reentrant>true</reentrant>
        </entity>
    </enterprise-beans>
</ejb-jar>
